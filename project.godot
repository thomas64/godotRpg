config_version=5

[application]

config/name="godotRpg"
run/main_scene="res://menu/menu_screen.tscn"
config/features=PackedStringArray("4.0", "Mobile")
config/icon="res://assets/icon.svg"

[audio]

buses/default_bus_layout="res://assets/resources/default_bus_layout.tres"

[autoload]

SettingsLoader="*res://scripts/autoload/settings_loader.gd"
TempStorage="*res://scripts/autoload/temp_storage.gd"
AudioManager="*res://scenes/audio_manager.tscn"
TransitionScreen="*res://scenes/transition_screen.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false
window/stretch/mode="canvas_items"

[editor]

import/use_multiple_threads=false

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

